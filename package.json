{
  "name": "sn-controls-aurelia",
  "version": "2.0.0-development.5",
  "description": "Aurelia controls for sensenet ECM",
  "main": "./dist/src/index.js",
  "typings": "./src/sn-controls-aurelia.d.ts",
  "scripts": {
    "lint": "node tslint --project ./tsconfig.json",
    "clean": "rimraf dist",
    "commit": "git-cz",
    "pretest": "npm run build",
    "test": "nyc mocha -p tsconfig.json dist/test/index.js",
    "check-coverage": "istanbul check-coverage ",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "postbuild": "./node_modules/.bin/cpx \"src/**/*.html\" dist/src && node-sass src -o dist/src",
    "publish:development": "npm publish --tag development",
    "typedoc": "./node_modules/.bin/typedoc --mode modules --tsconfig ./tsconfig.typedoc.json --out documentation --excludePrivate --theme default --readme readme.md ./src"
  },
  "files": [
    "dist",
    "src",
    "test",
    "documentation",
    "coverage"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SenseNet/sn-controls-aurelia.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "aurelia",
    "controls",
    "components"
  ],
  "author": "SenseNet",
  "license": "GPL-2.0",
  "nyc": {
    "exclude": "dist/test/**/*.*",
    "include": "dist/src/**/*.*",
    "check-coverage": true,
    "cache": true,
    "all": true,
    "reporter": [
      "lcov",
      "text-summary",
      "json"
    ]
  },
  "bugs": {
    "url": "https://github.com/SenseNet/sn-controls-aurelia/issues"
  },
  "homepage": "https://sensenet.com",
  "czConfig": {
    "path": "sensenet-kfi-cz-conventional-changelog"
  },
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/lodash": "^4.14.85",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "aurelia-bootstrapper": "^2.1.1",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-pal-nodejs": "^1.0.0-beta.2.0.0",
    "aurelia-templating": "^1.6.0",
    "aurelia-templating-binding": "^1.4.0",
    "aurelia-testing": "^1.0.0-beta.4.0.0",
    "chai": "^4.1.2",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.0",
    "mocha-typescript": "^1.1.11",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typedoc-default-themes": "^0.5.0",
    "typedoc-plugin-external-module-name": "^1.0.9",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "@types/quill": "^1.3.3",
    "aurelia-event-aggregator": "^1.0.1",
    "aurelia-framework": "^1.1.5",
    "aurelia-polyfills": "^1.2.2",
    "aurelia-router": "^1.4.0",
    "aurelia-validation": "^1.1.2",
    "cpx": "^1.5.0",
    "lodash": "^4.17.4",
    "material-components-web": "^0.25.0",
    "moment": "^2.19.2",
    "moment-timezone": "^0.5.13",
    "node-sass": "^4.6.1",
    "nyc": "^11.3.0",
    "quill": "^1.3.4",
    "rxjs": "^5.5.2",
    "sensenet-kfi-cz-conventional-changelog": "^1.0.0",
    "sn-client-js": "^3.0.0"
  }
}
